    <body>

     <div class="banner" style="background-image: linear-gradient(-225deg, rgba(0,101,168,0.6) 0%, rgba(0,36,61,0.6) 50%), url('background.jpg');">
      <div class="banner-content">
        <h1>Some Stuff</h1>
        <p>Very cool stuff, useful and smart</p>
        <%= link_to "GET IT", "http://www.airbnb.com", class: "btn btn-primary btn-lg" %>
      </div>
    </div>
    <!-- Your features code -->
    <div class="wrapper wrapper-center">
      <div class="container">
        <div class="row">
          <div class="col-xs-12 col-sm-6 col-lg-3">
            <card>
              <%= image_tag "globe.png", class: "img-small" %>
              <h2>Discover the World</h2>
              <p>The earth is your new home.</p>
            </card>
          </div>
          <div class="col-xs-12 col-sm-6 col-lg-3">
            <card>
              <%= image_tag "dollar.png", class: "img-small" %>
              <h2>Discover the World</h2>
              <p>The earth is your new home.</p>
            </card>
          </div>
          <div class="col-xs-12 col-sm-6 col-lg-3">
            <card>
              <%= image_tag "beer.png", class: "img-small" %>
              <h2>Discover the World</h2>
              <p>The earth is your new home.</p>
            </card>
          </div>
          <div class="col-xs-12 col-sm-6 col-lg-3">
            <card>
              <%= image_tag "heart.png", class: "img-small" %>
              <h2>Discover the World</h2>
              <p>The earth is your new home.</p>
            </card>
          </div>
        </div>
      </div>
    </div>

    <div class="wrapper wrapper-grey text-center">
      <h2>Your next trip</h2>
      <div class="container">
        <div class="row">
          <div class="col-xs-12 col-sm-4">
            <div class="card" style="background-image: url('./images/card-paris.jpg')">
              <h2>Paris</h2>
              <%= image_tag "romain.jpg", class: "avatar" %>
              <h1>Seb's home</h1>
              <p>Charming room</p>
            </div>
          </div>
          <div class="col-xs-12 col-sm-4">
            <div class="card" style="background-image: url('./images/card-brussels.jpeg')">
              <h2>Brussels</h2>
              <%= image_tag "seb.jpg", class: "avatar" %>
              <h1>Lovely place</h1>
              <p>Super awesome</p>
            </div>
          </div>
          <div class="col-xs-12 col-sm-4">
            <div class="card" style="background-image: url('./images/card-london.jpeg')">
              <h2>London</h2>
              <%= image_tag "anne.jpg", class: "avatar" %>
              <h1>Awesome digs</h1>
              <p>Really cool</p>
            </div>
          </div>
        </div>
      </div>
    </div>

    <!-- Your newsletter code -->

    <div class="wrapper wrapper-form">
      <div class="container">
        <div class="row">
          <div class="col-sm-offset-4 col-sm-4 text-center">
            <h2>Get news from us</h2>
            <p>Leave us your mail and get news from us</p>
            <form class="form-inline">
              <input type="text" class="form-control" placeholder="John Doe">
            </input>
            <button type="submit" class="btn btn-primary">Subscribe</button>
          </form>
        </div>
      </div>
    </div>
  </div>

